#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/readyapp (3)/node_modules/@grpc/proto-loader/build/bin/node_modules:/mnt/e/readyapp (3)/node_modules/@grpc/proto-loader/build/node_modules:/mnt/e/readyapp (3)/node_modules/@grpc/proto-loader/node_modules:/mnt/e/readyapp (3)/node_modules/@grpc/node_modules:/mnt/e/readyapp (3)/node_modules:/mnt/e/node_modules:/mnt/e/readyapp (3)/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/readyapp (3)/node_modules/@grpc/proto-loader/build/bin/node_modules:/mnt/e/readyapp (3)/node_modules/@grpc/proto-loader/build/node_modules:/mnt/e/readyapp (3)/node_modules/@grpc/proto-loader/node_modules:/mnt/e/readyapp (3)/node_modules/@grpc/node_modules:/mnt/e/readyapp (3)/node_modules:/mnt/e/node_modules:/mnt/e/readyapp (3)/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@grpc/proto-loader/build/bin/proto-loader-gen-types.js" "$@"
else
  exec node  "$basedir/../@grpc/proto-loader/build/bin/proto-loader-gen-types.js" "$@"
fi
